{
    "name" : "med/project",
    "description": "project description",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "baldinof/roadrunner-bundle": "^2.1",
        "composer/package-versions-deprecated": "1.11.99.5",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^2",
        "doctrine/orm": "^2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.5",
        "symfony/asset": "^5.4",
        "symfony/console": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/expression-language": "^5.4",
        "symfony/flex": "^1.3.1",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/intl": "^5.4",
        "symfony/mailer": "^5.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "^5.4",
        "symfony/polyfill-php80": "^1.15",
        "symfony/process": "^5.4",
        "symfony/property-access": "^5.4",
        "symfony/property-info": "^5.4",
        "symfony/runtime": "^5.3",
        "symfony/serializer": "^5.4",
        "symfony/string": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/web-link": "^5.4",
        "symfony/yaml": "^5.4",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "friendsofphp/php-cs-fixer": "^2.16",
        "liip/test-fixtures-bundle": "^1.0.0",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^5.4",
        "symfony/css-selector": "^5.4",
        "symfony/debug-bundle": "^5.4",
        "symfony/maker-bundle": "^1.5",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "^5.4",
        "symfony/web-profiler-bundle": "^5.4",
        "vimeo/psalm": "^3.8"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "schema:validate": "symfony console doctrine:schema:validate --skip-sync",
        "security:check": "symfony security:check",
        "twig:lint": "symfony console lint:twig templates/ --ansi",
        "yaml:lint": "symfony console lint:yaml config/ --ansi",
        "type:check": "psalm",
        "type:fix": "psalter --issues=all src",
        "cs:check": "php-cs-fixer fix --dry-run --ansi",
        "cs:fix": "php-cs-fixer fix --ansi",
        "test": "symfony php bin/phpunit",
        "check": [
            "@schema:validate",
            "@security:check",
            "@twig:lint",
            "@yaml:lint",
            "@type:check",
            "@cs:check",
            "@test"
        ],
        "up": [
            "docker-compose up -d",
            "symfony server:start -d",
            "symfony console cache:clear"
        ],
        "down": [
            "symfony server:stop",
            "docker-compose down"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^5.4"
        }
    }
}
